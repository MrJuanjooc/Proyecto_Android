<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.julian.graduatesfunlam">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logofunlam"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.RegisterActivity" />
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.example.julian.graduatesfunlam.Activity.AboutUsActivity"
            android:label="@string/activity_title_about_us" />
        <activity
            android:name="com.example.julian.graduatesfunlam.Activity.PrivacyPolicyActivity"
            android:label="@string/activity_title_privacy_policy" />
    </application>

</manifest>